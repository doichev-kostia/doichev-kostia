{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "baseUrl": ".",
    "verbatimModuleSyntax": true,
    "paths": {
      "~/*": [
        "./src/*"
      ]
    },
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      },
    ],
  },
  "exclude": [
    "dist",
    "node_modules",
  ],
  "include": [
    "./src/**/*",
    "./.astro/types.d.ts",
    "./sst-env.d.ts",
  ]
}
